IF := if
THEN := then
ELSE := else
END := end
REPEAT := repeat
UNTIL := until
READ := read
WRITE := write
ADD := \+
SUB := \-
MUL := \*
DIV := /
LP := \(
RP := \)
EQ := \=
LT := <
GT := >
QT := "
EOS := ;
ASSIGN := :\=
$NUMBER := [0-9]+
$IDENTIFIER := [a-zA-Z]+
$TEXT := [^"]+
& := &